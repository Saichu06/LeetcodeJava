class Solution {
    public int findSpecialInteger(int[] arr) {
        int c=1;
        int ele=arr[0];
        int n=arr.length;
        for(int i=1;i<arr.length;i++){
            if(ele==arr[i]) c=c+1;
            else{
                c=1;
                ele=arr[i];
            }

            if(c>(n/4)) return ele;
        }

        return ele;
    }
}
